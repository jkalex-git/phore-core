{
  "name": "phore/core",
  "description": "Phore core library",
  "license": "MIT",
  "homepage": "https://infracamp.org/phore/",
  "authors": [
    { "name": "Matthias Leuffen", "email": "m@tth.es" }
  ],
  "repositories": [
  ],
  "require": {
    "php": ">=7.1"
  },
  "suggest": {
    "ext-yaml": "Yaml encoding parsing",
    "ext-sodium": "Encryption functions",
    "ext-json": "Json encoding parsing"
  },
  "autoload": {
    "psr-4": {
      "Phore\\Core\\": "src/"
    },
    "files": [
      "src/functions.inc.php"
    ]
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "*",
    "ext-yaml": "*",
    "ext-sodium": "*",
    "ext-json": "*"
  }
}
